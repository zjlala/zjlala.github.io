#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepr_2916e7ca3eb46e52e06ce70c92555a49/node_modules/vuepress/bin/node_modules:/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepr_2916e7ca3eb46e52e06ce70c92555a49/node_modules/vuepress/node_modules:/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepr_2916e7ca3eb46e52e06ce70c92555a49/node_modules:/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepr_2916e7ca3eb46e52e06ce70c92555a49/node_modules/vuepress/bin/node_modules:/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepr_2916e7ca3eb46e52e06ce70c92555a49/node_modules/vuepress/node_modules:/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepr_2916e7ca3eb46e52e06ce70c92555a49/node_modules:/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vuepress-vite.js" "$@"
else
  exec node  "$basedir/../../bin/vuepress-vite.js" "$@"
fi
