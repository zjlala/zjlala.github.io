#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/@vuepress+plugin-redirect@2_e92c38ef75bc37d2b8f501cef06d92ec/node_modules/@vuepress/plugin-redirect/lib/cli/node_modules:/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/@vuepress+plugin-redirect@2_e92c38ef75bc37d2b8f501cef06d92ec/node_modules/@vuepress/plugin-redirect/lib/node_modules:/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/@vuepress+plugin-redirect@2_e92c38ef75bc37d2b8f501cef06d92ec/node_modules/@vuepress/plugin-redirect/node_modules:/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/@vuepress+plugin-redirect@2_e92c38ef75bc37d2b8f501cef06d92ec/node_modules/@vuepress/node_modules:/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/@vuepress+plugin-redirect@2_e92c38ef75bc37d2b8f501cef06d92ec/node_modules:/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/@vuepress+plugin-redirect@2_e92c38ef75bc37d2b8f501cef06d92ec/node_modules/@vuepress/plugin-redirect/lib/cli/node_modules:/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/@vuepress+plugin-redirect@2_e92c38ef75bc37d2b8f501cef06d92ec/node_modules/@vuepress/plugin-redirect/lib/node_modules:/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/@vuepress+plugin-redirect@2_e92c38ef75bc37d2b8f501cef06d92ec/node_modules/@vuepress/plugin-redirect/node_modules:/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/@vuepress+plugin-redirect@2_e92c38ef75bc37d2b8f501cef06d92ec/node_modules/@vuepress/node_modules:/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/@vuepress+plugin-redirect@2_e92c38ef75bc37d2b8f501cef06d92ec/node_modules:/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/cli/index.js" "$@"
else
  exec node  "$basedir/../../lib/cli/index.js" "$@"
fi
