#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/@vuepress+cli@2.0.0-rc.24/node_modules/@vuepress/cli/bin/node_modules:/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/@vuepress+cli@2.0.0-rc.24/node_modules/@vuepress/cli/node_modules:/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/@vuepress+cli@2.0.0-rc.24/node_modules/@vuepress/node_modules:/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/@vuepress+cli@2.0.0-rc.24/node_modules:/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/@vuepress+cli@2.0.0-rc.24/node_modules/@vuepress/cli/bin/node_modules:/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/@vuepress+cli@2.0.0-rc.24/node_modules/@vuepress/cli/node_modules:/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/@vuepress+cli@2.0.0-rc.24/node_modules/@vuepress/node_modules:/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/@vuepress+cli@2.0.0-rc.24/node_modules:/mnt/d/web-front-workspace/demostudio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@vuepress+cli@2.0.0-rc.24/node_modules/@vuepress/cli/bin/vuepress.js" "$@"
else
  exec node  "$basedir/../../../../../@vuepress+cli@2.0.0-rc.24/node_modules/@vuepress/cli/bin/vuepress.js" "$@"
fi
